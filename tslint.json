{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "ban-types": false,
    "member-access": [true, "no-public"],
    "no-empty": {
      "severity": "warning"
    },
    "quotemark": [true, "single"],
    "variable-name": [true, "allow-leading-underscore"],
    "unified-signatures": false
  },
  "rulesDirectory": []
}
